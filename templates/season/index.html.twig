{% extends 'base.html.twig' %}

{% block title %}{{page_title}} Donkey Series{% endblock %}

{% block body %}


<div class="container-fluid">
    <h1>{{page_title}}</h1>

   {% for season in seasons %}
    <div class="row">
        <div class="col-md-12">
            <ul>
                <li><a href="{{path('app_season_show', {'id': season.id})}}">{{season.number}}</a></li>
            </ul>
        </div>
    {% endfor %}
</div>
{% endblock %}
